{
  "hosting": {
    "public": "dist/ng-kite-autotrader",
    "rewrites":[{
      "source": "**",
      "destination": "/index.html"
    }, {
      "source": "/api/:path*",
      "destination": "https://kiteautotrader.herokuapp.com/api/:path"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "pubsub": {
      "port": 8085
    },
    "ui": {
      "enabled": true
    }
  }
}
